<!-- /wp:kadence/advancedheading -->

<!-- wp:kadence/advancedheading {"uniqueID":"10_bf8a0d-2f","align":"center","color":"palette2","typography":"Poppins","googleFont":true,"fontSubset":"latin","fontVariant":"500","fontWeight":"500","padding":["0","sm","0","sm"],"markBorder":"","markBorderStyles":[{"top":[null,"",""],"right":[null,"",""],"bottom":[null,"",""],"left":[null,"",""],"unit":"px"}],"tabletMarkBorderStyles":[{"top":[null,"",""],"right":[null,"",""],"bottom":[null,"",""],"left":[null,"",""],"unit":"px"}],"mobileMarkBorderStyles":[{"top":[null,"",""],"right":[null,"",""],"bottom":[null,"",""],"left":[null,"",""],"unit":"px"}],"colorClass":"theme-palette2","htmlTag":"p","fontSize":[20,18,20],"fontHeight":[1.3,"",1.3],"maxWidth":[911,"",""]} -->
